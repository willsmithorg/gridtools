import unittest
import pandas as pd
import numpy as np
from ..Column import Column
from ..AddDerivedColumns import AddDerivedColumns

import logging
logging.basicConfig(level=logging.INFO, datefmt='%H:%M:%S', format='%(asctime)s.%(msecs)03d - %(filename)s:%(lineno)d - %(message)s')


class TestColumn(unittest.TestCase):

    def setUp(self):
        self.col1 = Column(pd.Series(['abc','def', 'ghij', 'abc'], name='ser1'))
        self.col2 = Column(pd.Series([1, 2, 4],                    name='ser2'))
    


    def testLenOnNumeric(self):
        adc = AddDerivedColumns()
        adc.Register('Len')

        newcols = adc.Process(self.col2)
        
        self.assertEqual(len(newcols), 0)



if __name__ == '__main__':
    unittest.main()
    

